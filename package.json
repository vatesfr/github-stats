{
  "private": true,
  "name": "github-stats",
  "version": "0.0.0",
  "license": "ISC",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/vatesfr/github-stats",
  "bugs": "https://github.com/vatesfr/github-stats/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/vatesfr/github-stats"
  },
  "author": {
    "name": "Badr Azizbi",
    "email": ""
  },
  "preferGlobal": false,
  "main": "dist/cli",
  "bin": {
    "github-stats": "dist/cli"
  },
  "files": [
    "dist/"
  ],
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "exec-promise": "^0.6.1",
    "lodash": "^4.16.4",
    "promise-toolbox": "^0.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.0.0",
    "babel-plugin-lodash": "^3.2.9",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-babili": "^0.0.11",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.2",
    "dependency-check": "^2.6.0",
    "ghooks": "^1.3.2",
    "standard": "^8.4.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=dist/ src/",
    "depcheck": "dependency-check ./package.json",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=dist/ src/",
    "lint": "standard",
    "posttest": "npm run lint && npm run depcheck",
    "prepublish": "npm run build"
  },
  "babel": {
    "env": {
      "production": {
        "comments": false,
        "presets": [
          "babili"
        ]
      }
    },
    "plugins": [
      "lodash",
      "transform-runtime"
    ],
    "presets": [
      "latest",
      "stage-0"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ],
    "parser": "babel-eslint"
  },
  "config": {
    "ghooks": {
      "commit-msg": "npm test"
    }
  }
}
